  "author": "Jacano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jcanno/best-queue/issues"
  },
  "homepage": "https://github.com/Jcanno/best-queue#readme",
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.1.0",
    "prettier": "^2.4.1",
    "rollup": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^26.1.1",
    "typescript": "^4.4.3"
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec\\.[tj]s)$",
    "moduleFileExtensions": [
