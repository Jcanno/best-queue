{
  "name": "queue-request",
  "version": "2.0.0",
  "description": "Use queue to handle request task, base on Axios",
  "main": "queue-request.js",
  "scripts": {
    "release": "npm run build && npm publish",
    "release:beta": "npm run build && npm publish --tag=beta",
    "build": "rollup index.js --file queue-request.js --format umd --name 'Queue'",
    "commit": "git-cz",
    "test": "npm run build && mocha",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "request",
    "request-queue",
    "promise",
		"promise-queue",
		"axios",
		"axios-queue",
		"queue-request",
		"queue-promise"
  ],
  "author": "Jacano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jcanno/queue-request/issues"
  },
  "homepage": "https://github.com/Jcanno/queue-request#readme",
  "devDependencies": {
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^6.8.0",
    "eslint-config-scales": "^0.2.0",
    "mocha": "^7.1.1",
    "rollup": "^2.2.0"
  },
  "dependencies": {
    "axios": "^0.19.2"
  }
}
